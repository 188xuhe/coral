{
  "name": "coral",
  "group": "com.linkedin.@spec.name@",
  "version": "0.0.*",
  "scmUrl": "ssh://git.corp.linkedin.com:29418/coral/coral.git",
  "comment": "",
  "description": "Framework for portable data analysis",
  "watchers": [],
  "build": {
    "commands": {
      "test": "echo 'testing done by build command'",
      "snapshot": "ligradle -Psnapshot=true build",
      "build": "ligradle -Prelease=true -PallArtifacts build"
    },
    "toolchains": {
      "gradle": {
        "version": "5.2.1",
        "plugins": {
          "ligradle-jvm": {
            "libraries": [
              "product-plugins"
            ],
            "version": "3.*.*"
          },
          "shadow": "com.github.jengelman.gradle.plugins:shadow:4.0.4",
          "buildScans": "com.gradle:build-scan-plugin:2.1"
        }
      }
    },
    "type": "gradle",
    "versions": {
      "java": "1.8.0_121"
    }
  },
  "runtime": {},
  "review": {
    "wcTest": true
  },
  "codecoveragecheckinprecommit": {},
  "topology": {
    "applications": {}
  },
  "archive": {
    "artifacts": {},
    "databases": {},
    "repo_name": "DDS"
  },
  "dependencyResolution": {},
  "external": {
    "antlr-runtime": "org.antlr:antlr-runtime:3.4",
    "avatica-core": "org.apache.calcite.avatica:avatica-core:1.10.0",
    "calcite-avatica": "org.apache.calcite:calcite-avatica:1.6.0",
    "gson": "com.google.code.gson:gson:2.8.1",
    "presto-parser": "com.facebook.presto:presto-parser:0.157.1.30",
    "protobuf-java": "com.google.protobuf:protobuf-java:3.3.0",
    "slf4j-api": "org.slf4j:slf4j-api:1.7.25",
    "slf4j-log4j12": "org.slf4j:slf4j-log4j12:1.7.25",
    "testng": "org.testng:testng:6.4"
  },
  "product": {
    "calcite-mp": {
      "libraries": [
        "calcite-core"
      ],
      "version": "1.14.0.*",
      "group": "com.linkedin.calcite"
    },
    "hadoop": {
      "libraries": [
        "hadoop-auth",
        "hadoop-common",
        "hadoop-mapreduce-client-common"
      ],
      "version": "2.6.1.*"
    },
    "hive": {
      "libraries": [
        "hive-common",
        {
          "name": "hive-exec",
          "classifier": "core"
        },
        "hive-metastore",
        "hive-serde"
      ],
      "version": "1.1.0.136"
    }
  },
  "trunkDev": {
    "autoRevert": true
  },
  "extSCM": {}
